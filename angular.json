{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "newProjectRoot": "projects",
  "projects": {
    "celeste-search": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "configurations": {
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "serviceWorker": "ngsw-config.json"
            }
          },
          "options": {
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              {
                "glob": "robots.txt",
                "input": "src",
                "output": "."
              },
              {
                "glob": "robots.prod.txt",
                "input": "src",
                "output": "."
              },
              {
                "glob": "sitemap.xml",
                "input": "src",
                "output": "."
              }
            ],
            "index": "src/index.html",
            "outputPath": {
              "base": "dist/celeste-search"
            },
            "polyfills": [
              "src/polyfills.ts"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src/styles"
              ]
            },
            "styles": [
              "src/styles/styles.scss"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts"
          },
          "defaultConfiguration": ""
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "celeste-search:build"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "celeste-search:build:production"
            }
          },
          "options": {
            "buildTarget": "celeste-search:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/assets"
            ],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src/styles"
              ]
            },
            "styles": [
              "src/styles/styles.scss"
            ],
            "tsConfig": "src/tsconfig.spec.json"
          }
        }
      },
      "prefix": "cis",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "celeste-search-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "celeste-search:serve:production",
              "port": 8000
            }
          },
          "options": {
            "devServerTarget": "celeste-search:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "version": 1,
  "cli": {
    "analytics": false
  }
}