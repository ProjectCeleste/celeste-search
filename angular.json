{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "celeste-search",
  "newProjectRoot": "projects",
  "projects": {
    "celeste-search": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/netlify.toml",
              "src/robots.txt",
              "src/sitemap.xml"
            ],
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist/celeste-search",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src/styles"
              ]
            },
            "styles": [
              "src/styles/styles.scss"
            ],
            "tsConfig": "src/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "celeste-search:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json",
              "db/tsconfig.json"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "celeste-search:build:production",
              "progress": false
            }
          },
          "options": {
            "browserTarget": "celeste-search:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/netlify.toml",
              "src/robots.txt",
              "src/sitemap.xml"
            ],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules",
                "src/styles"
              ]
            },
            "styles": [
              "src/styles/styles.scss"
            ],
            "tsConfig": "src/tsconfig.spec.json"
          }
        }
      },
      "prefix": "cis",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "celeste-search-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "celeste-search:serve:production",
              "port": 8000
            }
          },
          "options": {
            "devServerTarget": "celeste-search:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": "e2e/tsconfig.e2e.json"
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "version": 1
}
